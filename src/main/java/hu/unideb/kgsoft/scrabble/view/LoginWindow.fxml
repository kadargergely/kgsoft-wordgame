<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane layoutX="14.0" layoutY="14.0" prefHeight="136.0" prefWidth="360.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="40.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="174.0" minWidth="10.0" prefWidth="143.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="257.0" minWidth="10.0" prefWidth="217.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="usernameLabel" prefHeight="13.0" prefWidth="120.0" text="Felhasználónév:" />
            <Label fx:id="passwordLabel" text="Jelszó:" GridPane.rowIndex="1" />
            <Label fx:id="passwordAgainLabel" text="Jelszó ismét:" GridPane.rowIndex="2" />
            <TextField fx:id="usernameField" GridPane.columnIndex="1" />
            <CheckBox fx:id="newUserCheckBox" onAction="#checkBoxClicked" mnemonicParsing="false" text="Új felhasználó" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <PasswordField fx:id="passwordField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <PasswordField fx:id="passwordAgainField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
      <GridPane alignment="CENTER" layoutX="42.0" layoutY="196.0" prefHeight="30.0" prefWidth="259.0" AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="80.0" AnchorPane.rightAnchor="80.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="loginButton" onAction="#loginButtonClicked" alignment="CENTER" mnemonicParsing="false" prefHeight="25.0" prefWidth="80.0" text="Belépés" />
            <Button fx:id="exitButton" onAction="#exitButtonClicked" mnemonicParsing="false" prefWidth="80.0" text="Kilépés" GridPane.columnIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
